{
  "name": "qq-farm-bot-ui",
  "scripts": {
    "dev:backend": "pnpm -C backend dev",
    "dev:frontend": "pnpm -C frontend dev",
    "dev": "pnpm build:frontend && pnpm dev:backend",
    "lint:frontend": "pnpm -C frontend lint",
    "build:frontend": "pnpm -C frontend build",
    "build": "pnpm build:frontend",
    "package:win": "pnpm build:frontend && pnpm -C backend build:win",
    "package:linux": "pnpm build:frontend && pnpm -C backend build:linux",
    "package:mac": "pnpm build:frontend && pnpm -C backend build:mac",
    "package:release": "pnpm build:frontend && pnpm -C backend build:release",
    "install:all": "pnpm install -r"
  },
  "devDependencies": {
  }
}
